<div class="col-sm-9 col-sm-offset-1 col-md-9 col-md-offset-3 main fixed">
	<div id="loading">
		<div class="loading-inner">
			<img class="loading-image" src="/images/spinner.gif" alt="Loading" />
	    	<b><h1 style="text-align:center;">Processing...</h1></b>
		</div>
	</div>
	<% include ../partials/messages %>
	<div class="dashContent">
		<div class="centered-text">
			<h1 class="page-header"><%= model.page.title %></h1>
		</div>
		<hr style="border-color:#561838;">
		<% var key = model.page.configurations.key; %>
		<% if (key == "home") { %>
			<% include ./dash/home %>
		<% } else if (key.indexOf("settings") > -1) { %>
			<% include ./dash/settings %>
		<% } else if (key == "news.new") { %>
			<% include ./dash/news-new %>
		<% } else if (key == "community.new") { %>
			<% include ./dash/community-new %>
		<% } else if (key == "event.new") { %>
			<% include ./dash/event-new %>
		<% } else if (key == "group.post") { %>
			<% include ./dash/group-post %>
		<% } %>
	</div>
</div>
<script type="text/javascript">
	$('form').areYouSure(
      {
        message: 'It looks like you have been editing something. '
               + 'If you leave before saving, your changes will be lost.'
      }
    );
</script>